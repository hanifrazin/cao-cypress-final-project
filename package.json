{
  "name": "cao-cypress-final-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "merge-reports": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress/reports/mochawesome.json",
    "generate-report": "marge cypress/reports/mochawesome.json --reportDir cypress/reports/html",
    "report": "npm run merge-reports && npm run generate-report",
    "test:all": "npx cypress run --browser chrome --headless --reporter mochawesome --reporter-options reportDir=cypress/reports/mochawesome-report,json=true && npm run merge-reports && npm run generate-report",
    "test:youtube": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/youtube/youtube.cy.js\" --reporter mochawesome --reporter-options reportDir=cypress/reports/mochawesome-report,json=true",
    "test:amazon": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/amazon/amazon.cy.js\" --reporter mochawesome --reporter-options reportDir=cypress/reports/mochawesome-report,json=true",
    "test:agoda": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/agoda/agoda.cy.js\" --reporter mochawesome --reporter-options reportDir=cypress/reports/mochawesome-report,json=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanifrazin/cao-cypress-final-project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/hanifrazin/cao-cypress-final-project/issues"
  },
  "homepage": "https://github.com/hanifrazin/cao-cypress-final-project#readme",
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "cypress": "^14.4.0",
    "cypress-xpath": "^2.0.1",
    "luxon": "^3.6.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
